{
  "name": "react-schedule-view",
  "version": "0.2.1",
  "description": "A zero-dependency, fully customizable component for displaying schedules in a daily or week format",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "author": "Troy DeSpain",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "rollup --config && cp -rf package.json ./build",
    "dist": "cd build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/despaintroy/react-schedule-view"
  },
  "keywords": [
    "react",
    "typescript",
    "schedule",
    "calendar",
    "view",
    "display",
    "daily",
    "week",
    "google calendar",
    "apple calendar"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/react": "^18.0.14",
    "prettier-plugin-organize-imports": "^3.0.0",
    "react": "^18.2.0",
    "rollup": "^2.75.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.32.1",
    "type-fest": "^2.13.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  }
}
